{
  "name": "cs-training-tool",
  "version": "0.2.3",
  "description": "COREsea Coral Reef Monitoring Training Tool ",
  "repository": "https://github.com/victoria-fahey/cs-training-tool.git",
  "main": "server/index.js",
  "author": "Victoria Fahey",
  "license": "ISC",
  "scripts": {
    "client": "webpack serve --config ./client/webpack.config.js --hot",
    "dev": "run-p dev:client dev:server",
    "dev:client": "npm run webpack -- --watch",
    "dev:server": "nodemon server",
    "start": "node server",
    "build": "run-p build:client build:server",
    "build:client": "npm run webpack -- --mode production",
    "build:server": "npm run knex -- migrate:latest",
    "server": "node server",
    "test": "jest --watchAll",
    "webpack": "webpack --config ./client/webpack.config.js",
    "lint": "eslint --ext .js,.jsx .",
    "knex": "knex --knexfile ./server/db/knexfile.js"
  },
  "engines": {
    "node": "14.x"
  },
  "keywords": [
    "frontend"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-runtime"
    ]
  },
  "eslintConfig": {
    "extends": "eda",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "@chakra-ui/color-mode": "<1.2.0",
    "@chakra-ui/icons": "^1.1.1",
    "@chakra-ui/react": "^1.7.3",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "express": "^4.17.1",
    "framer-motion": "^4.1.17",
    "knex": "^0.21.13",
    "react-icons": "^4.3.1",
    "react-lorem-ipsum": "^1.4.9",
    "react-router": "^6.1.1",
    "react-router-dom": "^6.0.2",
    "sqlite3": "^5.0.2",
    "superagent": "6.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.13.1",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "7.13.0",
    "@babel/plugin-transform-runtime": "7.13.1",
    "@babel/preset-env": "7.13.0",
    "@babel/preset-react": "7.12.13",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "11.2.5",
    "babel-loader": "^8.2.2",
    "eslint": "7.9.0",
    "eslint-config-eda": "0.1.4",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^2.4.5",
    "nock": "^13.2.1",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "pg": "^8.5.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "supertest": "^6.1.6",
    "webpack": "5.24.0",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
